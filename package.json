{
  "name": "@cloudcue/plugin-mock-ts",
  "version": "1.0.8",
  "description": "TypeScript mock plugin for CloudCue testing",
  "author": "Nick <nlaurie@metafig.com>",
  "license": "ISC",
  "cloudcue": {
    "id": "com.cloudcue.mock-ts"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudcue/cloudcue-plugins.git"
  },
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "lint": "eslint 'src/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bugs": {
    "url": "https://github.com/cloudcue/cloudcue-plugins/issues"
  },
  "homepage": "https://github.com/cloudcue/cloudcue-plugins#readme",
  "dependencies": {
    "@cloudcue/sdk": "*"
  },
  "devDependencies": {
    "@cloudcue/eslint-config": "*",
    "@cloudcue/vite-config": "*",
    "@types/node": "^20.11.20",
    "eslint": "^9.15.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.4.3"
  }
}
